msgctxt "ActivityInfo|"
msgid "Complete the arrangement of fruits"
msgstr ""

#: activities/algorithm/ActivityInfo.qml:29
msgctxt "ActivityInfo|"
msgid "Logic training activity"
msgstr ""

#: activities/algorithm/ActivityInfo.qml:30
msgctxt "ActivityInfo|"
